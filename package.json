{
  "name": "nui",
  "version": "0.0.1",
  "description": "A site manager new ui",
  "scripts": {
    "typings-install": "typings prune && typings install",
    "postinstall": "npm run typings-install",
    "build": "webpack --inline --colors --progress --display-error-details --display-cached",
    "postbuild": "node postbuild.js main polyfills vendor > ./dist/index.html",
    "watch": "npm run build -- --watch",
    "server": "webpack-dev-server --inline --colors --progress --display-error-details --display-cached --port 3000  --content-base src",
    "start": "npm run server",
    "test": "karma start karma.conf.js"
  },
  "devDependencies": {
    "angular2-template-loader": "^0.4.0",
    "autoprefixer": "^6.3.7",
    "awesome-typescript-loader": "~0.17.0",
    "compass-mixins": "^0.12.10",
    "css-loader": "^0.23.1",
    "es6-promise": "3.0.2",
    "es6-shim": "0.35.0",
    "file-loader": "^0.9.0",
    "html-loader": "^0.4.3",
    "jasmine-core": "^2.4.1",
    "json-loader": "^0.5.4",
    "karma": "^1.1.1",
    "karma-chrome-launcher": "^1.0.1",
    "karma-jasmine": "^1.0.2",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^1.7.0",
    "node-sass": "^3.8.0",
    "postcss-loader": "^0.9.1",
    "raw-loader": "^0.5.1",
    "reflect-metadata": "0.1.2",
    "sass-loader": "^4.0.0",
    "source-map-loader": "^0.1.5",
    "style-loader": "^0.13.1",
    "to-string-loader": "^1.1.4",
    "typescript": "~1.8.9",
    "typings": "~1.0.3",
    "url-loader": "^0.5.7",
    "webpack": "^1.12.9",
    "webpack-dev-server": "^1.14.0",
    "webpack-merge": "^0.8.4"
  },
  "dependencies": {
    "@angular/common": "^2.0.0-rc.3",
    "@angular/compiler": "^2.0.0-rc.3",
    "@angular/core": "^2.0.0-rc.3",
    "@angular/http": "^2.0.0-rc.3",
    "@angular/platform-browser": "^2.0.0-rc.3",
    "@angular/platform-browser-dynamic": "^2.0.0-rc.3",
    "@angular/platform-server": "^2.0.0-rc.3",
    "@angular/router": "^3.0.0-beta.2",
    "@ngrx/core": "^1.0.1",
    "@ngrx/store": "^2.0.1",
    "core-js": "^2.2.0",
    "ie-shim": "^0.1.0",
    "normalize.css": "^4.2.0",
    "rxjs": "5.0.0-beta.6",
    "svg-sprite-loader": "~0.0.26",
    "socket.io-client":"1.3.6",
    "zone.js": "~0.6.12"
  }
}
